{
  "compilerOptions": {
    "target": "ES2022",
    "experimentalDecorators": true,
    "useDefineForClassFields": false,
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "skipLibCheck": true,
    "types": ["node"],
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "moduleDetection": "force",
    "allowJs": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["./*"]
    },
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["src", "src/pages/AddMemberForm.tsx", "src/services/smsApi.ts", "src/services/membersrc/components/MemberFormModal.tsxodalsrc/components/MembersTable.tsxasrc/utilsc/hooks, "hooks", "utils", "env.d.ts"]
}
